### et:InsurancePolicy
et:InsurancePolicy rdf:type owl:Class ;
	rdfs:label "Insurance Policy" ;
	rdfs:subClassOf 
		[ rdf:type owl:Restriction ;
		  owl:onProperty et:adheresTo ;
		  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		  owl:onClass et:Insurance ] ,
		[ rdf:type owl:Restriction ;
		  owl:onProperty et:adheresTo ;
		  owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		  owl:onClass et:Insurance ] ,
		[ rdf:type owl:Restriction ;
		  owl:onProperty et:associatedToPolicyLevel ;
		  owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		  owl:onClass et:PolicyLevelConcept ] .

