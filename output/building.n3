@prefix bot: <https://w3id.org/bot#> .
@prefix building: <http://bimerr.iot.linkeddata.es/def/building#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix props: <http://bimerr.iot.linkeddata.es/def/material-properties#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

building: a owl:Ontology ;
    dc:creator "MarÃ­a Poveda-VillalÃ³n",
        "Serge ChÃ¡vez-Feria" ;
    dc:title "Building Ontology" ;
    owl:versionInfo "0.0.3" .

building:BiomassBoiler a owl:Class ;
    rdfs:label "Biomass Boiler" ;
    rdfs:subClassOf building:Boiler .

building:CoolingSystem a owl:Class ;
    rdfs:label "Cooling System" ;
    rdfs:subClassOf building:System .

building:Door a owl:Class ;
    rdfs:label "Door" ;
    rdfs:subClassOf building:BuildingElement .

building:DuctSegment a owl:Class ;
    rdfs:label "Duct Segment" ;
    rdfs:subClassOf building:FlowSegment .

building:Fan a owl:Class ;
    rdfs:label "Fan" ;
    rdfs:subClassOf building:FlowMovingDevice .

building:FenestrationSystem a owl:Class ;
    rdfs:label "Fenestration System" ;
    rdfs:subClassOf building:System .

building:FossilBoiler a owl:Class ;
    rdfs:label "Fossil Boiler" ;
    rdfs:subClassOf building:Boiler .

building:HeatExchanger a owl:Class ;
    rdfs:label "Heat Exchanger" ;
    rdfs:subClassOf building:EnergyConversionDevice .

building:HeatingSystem a owl:Class ;
    rdfs:label "Heating System" ;
    rdfs:subClassOf building:System .

building:Outlet a owl:Class ;
    rdfs:label "Outlet" ;
    rdfs:subClassOf building:FlowTerminal .

building:PhotovoltaicSystem a owl:Class ;
    rdfs:label "Photovoltaic System" ;
    rdfs:subClassOf building:System .

building:PipeSegment a owl:Class ;
    rdfs:label "Pipe Segment" ;
    rdfs:subClassOf building:FlowSegment .

building:Pump a owl:Class ;
    rdfs:label "Pump" ;
    rdfs:subClassOf building:FlowMovingDevice .

building:Roof a owl:Class ;
    rdfs:label "Roof" ;
    rdfs:subClassOf building:BuildingElement .

building:Slab a owl:Class ;
    rdfs:label "Slab" ;
    rdfs:subClassOf building:BuildingElement .

building:SolarDevice a owl:Class ;
    rdfs:label "Solar Device" ;
    rdfs:subClassOf building:EnergyConversionDevice .

building:SolarSystem a owl:Class ;
    rdfs:label "Solar System" ;
    rdfs:subClassOf building:System .

building:SpaceHeater a owl:Class ;
    rdfs:label "Space Heater" ;
    rdfs:subClassOf building:FlowTerminal .

building:SwitchingDevice a owl:Class ;
    rdfs:label "Switching Device" ;
    rdfs:subClassOf building:FlowController .

building:Tank a owl:Class ;
    rdfs:label "Tank" ;
    rdfs:subClassOf building:FlowStorageDevice .

building:TubeBundle a owl:Class ;
    rdfs:label "Tube Bundle" ;
    rdfs:subClassOf building:EnergyConversionDevice .

building:VentilationSystem a owl:Class ;
    rdfs:label "Ventilation System" ;
    rdfs:subClassOf building:System .

building:Wall a owl:Class ;
    rdfs:label "Wall" ;
    rdfs:subClassOf building:BuildingElement .

building:Window a owl:Class ;
    rdfs:label "Window" ;
    rdfs:subClassOf building:BuildingElement .

building:Building a owl:Class ;
    rdfs:label "Building" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty building:ifcIdentifier ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty building:ifcIdentifier ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty bot:hasSimple3DModel ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty wgs84_pos:long ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty wgs84_pos:lat ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty wgs84_pos:alt ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty building:buildingAddress ],
        [ a owl:Restriction ;
            owl:allValuesFrom building:Storey ;
            owl:onProperty bot:hasStorey ],
        bot:Building .

building:DistributionElement a owl:Class ;
    rdfs:label "Distribution Element" ;
    rdfs:subClassOf building:Element .

building:FlowController a owl:Class ;
    rdfs:label "Flow Controller" ;
    rdfs:subClassOf building:DistributionFlowElement .

building:FlowStorageDevice a owl:Class ;
    rdfs:label "Flow Storage Device" ;
    rdfs:subClassOf building:DistributionFlowElement .

building:MaterialConstituent a owl:Class ;
    rdfs:label "Material Constituent" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom props:Material ;
            owl:onProperty building:hasMaterial ] .

building:MaterialConstituentSet a owl:Class ;
    rdfs:label "Material Constituent Set" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass building:MaterialConstituent ;
            owl:onProperty building:hasMaterialConstituent ] .

building:MaterialLayer a owl:Class ;
    rdfs:label "Material Layer" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:float ;
            owl:onProperty building:thickness ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:integer ;
            owl:onProperty building:position ],
        [ a owl:Restriction ;
            owl:allValuesFrom props:Material ;
            owl:onProperty building:hasMaterial ] .

building:MaterialLayerSet a owl:Class ;
    rdfs:label "Material Layer Set" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass building:MaterialLayer ;
            owl:onProperty building:hasMaterialLayer ] .

building:MaterialProfile a owl:Class ;
    rdfs:label "Material Profile" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom props:Material ;
            owl:onProperty building:hasMaterial ] .

building:MaterialProfileSet a owl:Class ;
    rdfs:label "Material Profile Set" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass building:MaterialProfile ;
            owl:onProperty building:hasMaterialProfile ] .

building:Space a owl:Class ;
    rdfs:label "Space" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty building:ifcIdentifier ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty building:ifcIdentifier ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:integer ;
            owl:onProperty building:maxOccupants ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:integer ;
            owl:onProperty building:minOccupants ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty bot:hasSimple3DModel ],
        [ a owl:Restriction ;
            owl:allValuesFrom building:Element ;
            owl:onProperty bot:hasElement ],
        bot:Space .

building:Storey a owl:Class ;
    rdfs:label "Storey" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty building:ifcIdentifier ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty building:ifcIdentifier ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty bot:hasSimple3DModel ],
        [ a owl:Restriction ;
            owl:onProperty bot:containsZone ;
            owl:someValuesFrom building:Apartment ],
        [ a owl:Restriction ;
            owl:allValuesFrom building:Apartment ;
            owl:onProperty building:hasApartment ],
        bot:Storey .

building:buildingAddress a owl:DatatypeProperty ;
    rdfs:label "building address" ;
    rdfs:domain building:Building ;
    rdfs:range xsd:string .

building:hasApartment a owl:ObjectProperty ;
    rdfs:label "has apartment" .

building:hasConstruction a owl:ObjectProperty ;
    rdfs:label "has construction" .

building:hasMaterialConstituent a owl:ObjectProperty ;
    rdfs:label "has material constituent" .

building:hasMaterialLayer a owl:ObjectProperty ;
    rdfs:label "has material layer" .

building:hasMaterialProfile a owl:ObjectProperty ;
    rdfs:label "has material profile" .

building:includesElement a owl:ObjectProperty ;
    rdfs:label "includes element" .

building:isPartOfSystem a owl:ObjectProperty ;
    rdfs:label "is part of system" .

building:maxOccupants a owl:DatatypeProperty ;
    rdfs:label "max occupants" ;
    rdfs:range xsd:integer .

building:minOccupants a owl:DatatypeProperty ;
    rdfs:label "min occupants" ;
    rdfs:range xsd:integer .

building:position a owl:DatatypeProperty ;
    rdfs:label "position" ;
    rdfs:range xsd:integer .

building:thickness a owl:DatatypeProperty ;
    rdfs:label "thickness" ;
    rdfs:range xsd:float .

wgs84_pos:alt a owl:DatatypeProperty ;
    rdfs:label "alt" .

wgs84_pos:lat a owl:DatatypeProperty ;
    rdfs:label "lat" .

wgs84_pos:long a owl:DatatypeProperty ;
    rdfs:label "long" .

bot:Building a owl:Class ;
    rdfs:label "Building" .

bot:Element a owl:Class ;
    rdfs:label "Element" .

bot:Space a owl:Class ;
    rdfs:label "Space" .

bot:Storey a owl:Class ;
    rdfs:label "Storey" .

bot:Zone a owl:Class ;
    rdfs:label "Zone" .

bot:containsZone a owl:ObjectProperty ;
    rdfs:label "contains zone" .

bot:hasElement a owl:ObjectProperty ;
    rdfs:label "has element" .

bot:hasSpace a owl:ObjectProperty ;
    rdfs:label "has space" .

bot:hasStorey a owl:ObjectProperty ;
    rdfs:label "has storey" .

bot:hasSubElement a owl:ObjectProperty ;
    rdfs:label "has sub element" .

building:Apartment a owl:Class ;
    rdfs:label "Apartment" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty building:ifcIdentifier ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty building:ifcIdentifier ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty bot:hasSimple3DModel ],
        [ a owl:Restriction ;
            owl:allValuesFrom building:Space ;
            owl:onProperty bot:hasSpace ],
        bot:Zone .

building:Boiler a owl:Class ;
    rdfs:label "Boiler" ;
    rdfs:subClassOf building:EnergyConversionDevice .

building:FlowMovingDevice a owl:Class ;
    rdfs:label "Flow Moving Device" ;
    rdfs:subClassOf building:DistributionFlowElement .

building:FlowSegment a owl:Class ;
    rdfs:label "Flow Segment" ;
    rdfs:subClassOf building:DistributionFlowElement .

building:FlowTerminal a owl:Class ;
    rdfs:label "Flow Terminal" ;
    rdfs:subClassOf building:DistributionFlowElement .

building:hasMaterial a owl:ObjectProperty ;
    rdfs:label "has material" .

props:Material a owl:Class ;
    rdfs:label "Material" .

building:EnergyConversionDevice a owl:Class ;
    rdfs:label "Energy Conversion Device" ;
    rdfs:subClassOf building:DistributionFlowElement .

building:BuildingElement a owl:Class ;
    rdfs:label "Building Element" ;
    rdfs:subClassOf building:Element .

building:Element a owl:Class ;
    rdfs:label "Element" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty building:ifcIdentifier ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty building:ifcIdentifier ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty bot:hasSimple3DModel ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:unionOf ( building:MaterialLayerSet building:MaterialProfileSet building:MaterialConstituentSet ) ] ;
            owl:onProperty building:hasConstruction ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass building:System ;
            owl:onProperty building:isPartOfSystem ],
        [ a owl:Restriction ;
            owl:allValuesFrom building:Element ;
            owl:onProperty bot:hasSubElement ],
        bot:Element .

bot:hasSimple3DModel a owl:DatatypeProperty ;
    rdfs:label "has simple3 d model" ;
    rdfs:range xsd:string .

building:DistributionFlowElement a owl:Class ;
    rdfs:label "Distribution Flow Element" ;
    rdfs:subClassOf building:DistributionElement .

building:System a owl:Class ;
    rdfs:label "System" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass building:Element ;
            owl:onProperty building:includesElement ] .

building:ifcIdentifier a owl:DatatypeProperty ;
    rdfs:label "ifc identifier" ;
    rdfs:range xsd:string .

