@prefix : <http://theontology.namespace.com#> .
@prefix dc: <http://purl.org/dc/elements/1.1> .
@prefix kpi: <http://theontology.namespace.com#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix s4bldg: <http://reusedontologies.com#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://theontology.namespace.com#> .

<http://theontology.namespace.com#> rdf:type owl:Ontology ;
                                     dc:creator "Serge" ;
                                     owl:versionInfo "0.0.1" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1creator
dc:creator rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#datetime
xsd:datetime rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://theontology.namespace.com#group
kpi:group rdf:type owl:ObjectProperty ;
          rdfs:range [ owl:intersectionOf ( kpi:Class1
                                            kpi:Class2
                                          ) ;
                       rdf:type owl:Class
                     ] .


###  http://theontology.namespace.com#hasCategory
kpi:hasCategory rdf:type owl:ObjectProperty .


###  http://theontology.namespace.com#hasKPI
kpi:hasKPI rdf:type owl:ObjectProperty .


###  http://theontology.namespace.com#hasScenario
kpi:hasScenario rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://theontology.namespace.com#address
kpi:address rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain kpi:Project ;
            rdfs:range xsd:string .


###  http://theontology.namespace.com#changeDate
kpi:changeDate rdf:type owl:DatatypeProperty ;
               rdfs:domain kpi:Project ;
               rdfs:range xsd:datetime .


###  http://theontology.namespace.com#comment
kpi:comment rdf:type owl:DatatypeProperty ;
            rdfs:domain kpi:KPI ;
            rdfs:range xsd:string .


###  http://theontology.namespace.com#coordinates
kpi:coordinates rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain kpi:Project ;
                rdfs:range xsd:string .


###  http://theontology.namespace.com#identifier
kpi:identifier rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:range xsd:string .


###  http://theontology.namespace.com#kpiValue
kpi:kpiValue rdf:type owl:DatatypeProperty ;
             rdfs:domain kpi:KPI ;
             rdfs:range xsd:float .


###  http://theontology.namespace.com#name
kpi:name rdf:type owl:DatatypeProperty ;
         rdfs:range xsd:string .


###  http://theontology.namespace.com#unit
kpi:unit rdf:type owl:DatatypeProperty ;
         rdfs:domain kpi:KPI ;
         rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://theontology.namespace.com#Class1
kpi:Class1 rdf:type owl:Class ;
           owl:equivalentClass kpi:Class2 ,
                               kpi:KPI .


###  http://theontology.namespace.com#Class2
kpi:Class2 rdf:type owl:Class .


###  http://theontology.namespace.com#Comfort
kpi:Comfort rdf:type owl:Class ;
            rdfs:subClassOf kpi:KPICategory .


###  http://theontology.namespace.com#Economic
kpi:Economic rdf:type owl:Class ;
             rdfs:subClassOf kpi:KPICategory .


###  http://theontology.namespace.com#KPI
kpi:KPI rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty kpi:hasScenario ;
                          owl:someValuesFrom kpi:Scenario
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty kpi:hasCategory ;
                          owl:allValuesFrom kpi:KPICategory
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty kpi:hasCategory ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass kpi:KPICategory
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty kpi:hasScenario ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass kpi:Scenario
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty kpi:hasCategory ;
                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass kpi:KPICategory
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty kpi:hasScenario ;
                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass kpi:Scenario
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty kpi:identifier ;
                          owl:allValuesFrom xsd:string
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty kpi:name ;
                          owl:allValuesFrom xsd:string
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty kpi:kpiValue ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:float
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty kpi:unit ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:string
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty kpi:unit ;
                          owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:string
                        ] .


###  http://theontology.namespace.com#KPICategory
kpi:KPICategory rdf:type owl:Class ;
                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                  owl:onProperty kpi:group ;
                                  owl:someValuesFrom [ owl:intersectionOf ( kpi:Class1
                                                                            kpi:Class2
                                                                          ) ;
                                                       rdf:type owl:Class
                                                     ]
                                ] .


###  http://theontology.namespace.com#Project
kpi:Project rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty kpi:hasKPI ;
                              owl:someValuesFrom kpi:KPI
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty kpi:hasKPI ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass kpi:KPI
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty kpi:name ;
                              owl:someValuesFrom xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty kpi:identifier ;
                              owl:allValuesFrom xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty kpi:address ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty kpi:changeDate ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:datetime
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty kpi:coordinates ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty kpi:identifier ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty kpi:name ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty kpi:address ;
                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty kpi:changeDate ;
                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:datetime
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty kpi:coordinates ;
                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty kpi:identifier ;
                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty kpi:name ;
                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] .


###  http://theontology.namespace.com#Scenario
kpi:Scenario rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty kpi:identifier ;
                               owl:someValuesFrom xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty kpi:name ;
                               owl:allValuesFrom xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty kpi:identifier ;
                               owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty kpi:name ;
                               owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty kpi:identifier ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty kpi:name ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:string
                             ] .


###  http://theontology.namespace.com#SubCat1
kpi:SubCat1 rdf:type owl:Class ;
            rdfs:subClassOf kpi:KPICategory .


###  http://theontology.namespace.com#SubCat2
kpi:SubCat2 rdf:type owl:Class ;
            rdfs:subClassOf kpi:KPICategory .


#################################################################
#    Individuals
#################################################################

###  http://theontology.namespace.com#EnergyCost
kpi:EnergyCost rdf:type owl:NamedIndividual ,
                        kpi:Economic .


###  http://theontology.namespace.com#MaterialsCost
kpi:MaterialsCost rdf:type owl:NamedIndividual ,
                           kpi:Economic .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( kpi:Comfort
                kpi:Economic
                kpi:SubCat1
                kpi:SubCat2
              )
] .


